.bgmenu
{
	background-color: #f2c000;
	height: 42px;
}
.main_menu
{
	background: #00963c;
}
.main_menu>ul
{
	background-color: #00943a;
	float: left;
	list-style-type: none;
	margin: 0px;
	padding: 0px;
	width: 100%;
}
.main_menu>ul>li
{
	border-right: 1px dotted #fff;
	color: white;
	float: left;
	padding: 11px 27px;
	position: relative;
	text-align: center;
}
.main_menu>ul>li>a
{
	color: white;
	font-weight: bold;
	text-decoration: none;
}
.main_menu>ul>li:last-child
{
	border-right: none;
}
.main_menu>ul>li>ul
{
	background: #00943a;
	display: none;
	left: 0px;
	list-style: none;
	position: absolute;
	top: 41px;
	width: 198px;
	z-index: 10000;
}
.main_menu>ul>li:hover ul
{
	display: block;
}
.main_menu ul li:hover
{
	background: #50af2d;
	transition: all 0.25s;
}
.main_menu>ul>li>ul>li
{
	cursor: pointer;
	margin-left: -40px;
	padding: 10px 10px;
}
.main_menu>ul>li>ul>li
{
	border-bottom: 1px dotted #ACACAC;
	
}
.main_menu>ul>li>ul>li>a
{
	color: white;
	font-weight:bold;

	text-decoration: none;
	
}
.btnbutton{
	
}
.lbinfoproduct{
	color:black;
	font-weight:bold;
}
.btn-file {
    position: relative;
    overflow: hidden;
}
.btn-file input[type=file] {
    position: absolute;
    top: 0;
    right: 0;
    min-width: 100%;
    min-height: 100%;
    font-size: 100px;
    text-align: right; 
    filter: alpha(opacity=0);
    opacity: 0;
    outline: none;
    background: white;
    cursor: inherit;
    display: block;
}
.hr_menu{
	height:50px;
	background-color:green;
	
}
.content_view{
	overflow-x:scroll;
}
body
{
	
	
	font-family: Cambria, Palatino, "Palatino Linotype", "Palatino LT STD", Georgia, serif;
	font-size: 15px;
	font-weight: 400;
	overflow-y: scroll;
}
.header
{
	height: 130px;
}
.nn-company
{
	color: #781f19;
	font-family: Impact;
	font-size: 50px;
	padding-bottom: 2px;
	position: absolute;
	text-shadow: 0 0 2px yellow;
	top: 0px;
}
#linkregister
{
	background-color: #f2c000;
	border: 1px solid #228b22;
	border-style: dotted;
	color: #781f19;
	text-decoration: none;
}
#linkregister:hover
{
	background-color: #eee2b6;
	box-shadow: 10px 10px 5px #888888;
	color: #781f19;
}

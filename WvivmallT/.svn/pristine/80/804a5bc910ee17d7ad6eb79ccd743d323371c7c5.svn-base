							<option>05</option>
							<option>06</option>
							<option>07</option>
							<option>08</option>
							<option>09</option>
							<option>10</option>
							<option>11</option>
							<option>12</option>
							</select>
					</div>
					<div class="col-md-1">
					<button id="btnview" type="button" class="btn btn-success">Xem</button>
					</div>
				</div>
				<div class="row">
					<div class="col-md-11">
						<div id="chart"></div>
					</div>
				</div>
				<div class="col-md-10" id="table_content">
				</div>
			</div>
		</div>
